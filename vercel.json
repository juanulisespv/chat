{
  "version": 2,
  "routes": [
    { "src": "/api/consultar", "dest": "/api/consultar.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://juanulisespv.github.io/mac-os-classic/"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://juanulisespv.github.io/mac-os-classic/"
        }
      ]
    }
  ]
}
